<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="genlab.core.algo">
      <algo
            class="edf.simdiasca.algos.WritePopulationForSimdiascaAlgo"
            id="edf.simdiasca.algos.WritePopulationForSimdiascaAlgo">
      </algo>
   </extension>
   <extension
         point="genlab.core.doc">
