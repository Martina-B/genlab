      </library>
      <docalgo
            algo_id="edf.simdiasca.algos.WritePopulationForSimdiascaAlgo"
            description="Write a population as SimDiasca constructors"
            library_id="edf.simdiasca.library"
            memory_complexity="O(n)"
            time_complexity="O(n)">
      </docalgo>
   </extension>

